{
    "name": "cundd/fleet",
    "type": "typo3-cms-extension",
    "description": "Command line utility to provide system information for fleet",
    "homepage": "https://github.com/cundd/fleet_master/",
    "authors": [
        {
            "name": "Daniel Corn",
            "email": "info@cundd.net"
        }
    ],
    "license": "MIT",
    "keywords": [
        "typo3cms",
        "fleet",
        "service"
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "typo3/cms-core": "^12.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.10",
        "typo3/testing-framework": "^7 || ^8"
    },
    "autoload": {
        "psr-4": {
            "Cundd\\Fleet\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cundd\\Fleet\\Tests\\": "Tests/"
        }
    },
    "replace": {
        "typo3-ter/fleet": "self.version"
    },
    "extra": {
        "typo3/cms": {
            "app-dir": ".Build",
            "web-dir": ".Build/Web",
            "extension-key": "fleet"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    }
}
